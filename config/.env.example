# Telegram API Credentials
# Get these from https://my.telegram.org/apps
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here

# Telegram Phone Number (with country code, e.g., +1234567890)
TELEGRAM_PHONE=+1234567890

# Optional: Session name (default: telegram_session)
SESSION_NAME=telegram_session

# Optional: Minimum confidence score for signal extraction (0.0 to 1.0)
MIN_CONFIDENCE=0.75

# Optional: CSV output file path (supports environment variables like %TEMP%)
CSV_OUTPUT_PATH=%TEMP%\signal.csv

# Optional: Error log file path
ERROR_LOG_PATH=logs/extraction_errors.jsonl

# Optional: System log file path
SYSTEM_LOG_PATH=logs/system.log

# Optional: Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
